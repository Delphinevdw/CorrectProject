<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="416.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.gui.PassengerInfoController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Passenger info">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="67.0" text="First name" />
      <Label layoutX="15.0" layoutY="218.0" text="Date of birth (yyyy/mm/dd)" />
      <Label layoutX="15.0" layoutY="168.0" text="Gender" />
      <Label layoutX="15.0" layoutY="119.0" text="Last name" />
      <Label layoutX="15.0" layoutY="263.0" text="Passport number" />
      <Label layoutX="13.0" layoutY="304.0" text="Nationality" />
      <TextField fx:id="txtFirstName" layoutX="107.0" layoutY="63.0" />
      <TextField fx:id="txtLastName" layoutX="107.0" layoutY="115.0" />
      <CheckBox fx:id="CBMale" layoutX="107.0" layoutY="168.0" mnemonicParsing="false" text="M" />
      <CheckBox fx:id="CBFemale" layoutX="166.0" layoutY="168.0" mnemonicParsing="false" text="F" />
      <CheckBox fx:id="CBX" layoutX="226.0" layoutY="168.0" mnemonicParsing="false" text="X" />
      <TextField fx:id="txtDateOfBirth" layoutX="181.0" layoutY="214.0" />
      <TextField fx:id="txtPassportNumber" layoutX="107.0" layoutY="259.0" />
      <TextField fx:id="txtNationality" layoutX="107.0" layoutY="300.0" />
      <Button fx:id="submitBtn" layoutX="348.0" layoutY="360.0" mnemonicParsing="false" onAction="#loadMainbookerpane" text="Submit" />
      <CheckBox fx:id="CBMainbooker" layoutX="103.0" layoutY="351.0" mnemonicParsing="false" />
      <Label layoutX="13.0" layoutY="351.0" text="Mainbooker" />
   </children>
</AnchorPane>
